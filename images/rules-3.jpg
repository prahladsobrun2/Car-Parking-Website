<h2 data-start="22509" data-end="22570"> <code data-start="22514" data-end="22528">js/script.js</code> (validation + calculator + interactivity)</h2>
<pre class="overflow-visible! px-0!" data-start="22571" data-end="26079"><div class="contain-inline-size rounded-2xl corner-superellipse/1.1 relative bg-token-sidebar-surface-primary"><div class="flex items-center text-token-text-secondary px-4 py-2 text-xs font-sans justify-between h-9 bg-token-sidebar-surface-primary select-none rounded-t-2xl corner-t-superellipse/1.1">javascript</div><div class="sticky top-[calc(var(--sticky-padding-top)+9*var(--spacing))]"><div class="absolute end-0 bottom-0 flex h-9 items-center pe-2"><div class="bg-token-bg-elevated-secondary text-token-text-secondary flex items-center gap-4 rounded-sm px-2 font-sans text-xs"><button class="flex gap-1 items-center select-none py-1" aria-label="Copy"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" aria-hidden="true" class="icon-sm"><use href="/cdn/assets/sprites-core-c9exbsc1.svg#ce3544" fill="currentColor"></use></svg>Copy code</button></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-javascript"><span><span><span class="hljs-comment">// ============ Interactivity: hide/show services ============</span></span><span>
</span><span><span class="hljs-keyword">const</span></span><span> toggleServicesBtn = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"toggleServicesBtn"</span></span><span>);
</span><span><span class="hljs-keyword">const</span></span><span> servicesSection = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"servicesSection"</span></span><span>);

</span><span><span class="hljs-keyword">if</span></span><span> (toggleServicesBtn &amp;&amp; servicesSection) {
  toggleServicesBtn.</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">"click"</span></span><span>, </span><span><span class="hljs-function">() =&gt;</span></span><span> {
    </span><span><span class="hljs-keyword">const</span></span><span> isHidden = servicesSection.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">toggle</span></span><span>(</span><span><span class="hljs-string">"d-none"</span></span><span>);
    toggleServicesBtn.</span><span><span class="hljs-property">textContent</span></span><span> = isHidden ? </span><span><span class="hljs-string">"Show Services"</span></span><span> : </span><span><span class="hljs-string">"Hide Services"</span></span><span>;
  });
}

</span><span><span class="hljs-comment">// ============ Calculator ============</span></span><span>
</span><span><span class="hljs-keyword">const</span></span><span> calcBtn = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"calcBtn"</span></span><span>);
</span><span><span class="hljs-keyword">if</span></span><span> (calcBtn) {
  calcBtn.</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">"click"</span></span><span>, </span><span><span class="hljs-function">() =&gt;</span></span><span> {
    </span><span><span class="hljs-keyword">const</span></span><span> hours = </span><span><span class="hljs-title class_">Number</span></span><span>(</span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"hours"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> rate = </span><span><span class="hljs-title class_">Number</span></span><span>(</span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"rateType"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> resultBox = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"calcResult"</span></span><span>);

    </span><span><span class="hljs-keyword">if</span></span><span> (!hours || hours &lt;= </span><span><span class="hljs-number">0</span></span><span>) {
      resultBox.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">remove</span></span><span>(</span><span><span class="hljs-string">"d-none"</span></span><span>, </span><span><span class="hljs-string">"alert-info"</span></span><span>);
      resultBox.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">add</span></span><span>(</span><span><span class="hljs-string">"alert-warning"</span></span><span>);
      resultBox.</span><span><span class="hljs-property">textContent</span></span><span> = </span><span><span class="hljs-string">"Please enter a valid number of hours (min 1)."</span></span><span>;
      </span><span><span class="hljs-keyword">return</span></span><span>;
    }

    </span><span><span class="hljs-keyword">const</span></span><span> total = hours * rate;
    resultBox.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">remove</span></span><span>(</span><span><span class="hljs-string">"d-none"</span></span><span>, </span><span><span class="hljs-string">"alert-warning"</span></span><span>);
    resultBox.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">add</span></span><span>(</span><span><span class="hljs-string">"alert-info"</span></span><span>);
    resultBox.</span><span><span class="hljs-property">textContent</span></span><span> = </span><span><span class="hljs-string">`Estimated total: Rs <span class="hljs-subst">${total}</span></span></span><span> (</span><span><span class="hljs-subst">${hours}</span></span><span> hour(s)  Rs </span><span><span class="hljs-subst">${rate}</span></span><span>/hour)`;
  });
}

</span><span><span class="hljs-comment">// ============ Form Validation (validate at least 2 fields) ============</span></span><span>
</span><span><span class="hljs-keyword">const</span></span><span> contactForm = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"contactForm"</span></span><span>);
</span><span><span class="hljs-keyword">if</span></span><span> (contactForm) {
  contactForm.</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">"submit"</span></span><span>, </span><span><span class="hljs-function">(<span class="hljs-params">e</span></span></span><span>) =&gt; {
    e.</span><span><span class="hljs-title function_">preventDefault</span></span><span>();

    </span><span><span class="hljs-keyword">const</span></span><span> name = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"fullName"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>.</span><span><span class="hljs-title function_">trim</span></span><span>();
    </span><span><span class="hljs-keyword">const</span></span><span> email = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"email"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>.</span><span><span class="hljs-title function_">trim</span></span><span>();

    </span><span><span class="hljs-keyword">const</span></span><span> nameError = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"nameError"</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> emailError = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"emailError"</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> formMessage = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"formMessage"</span></span><span>);

    nameError.</span><span><span class="hljs-property">textContent</span></span><span> = </span><span><span class="hljs-string">""</span></span><span>;
    emailError.</span><span><span class="hljs-property">textContent</span></span><span> = </span><span><span class="hljs-string">""</span></span><span>;

    </span><span><span class="hljs-keyword">let</span></span><span> valid = </span><span><span class="hljs-literal">true</span></span><span>;

    </span><span><span class="hljs-comment">// Validate name (field 1)</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> (name.</span><span><span class="hljs-property">length</span></span><span> &lt; </span><span><span class="hljs-number">2</span></span><span>) {
      nameError.</span><span><span class="hljs-property">textContent</span></span><span> = </span><span><span class="hljs-string">"Name is required (min 2 characters)."</span></span><span>;
      valid = </span><span><span class="hljs-literal">false</span></span><span>;
    }

    </span><span><span class="hljs-comment">// Validate email format (field 2)</span></span><span>
    </span><span><span class="hljs-keyword">const</span></span><span> emailPattern = </span><span><span class="hljs-regexp">/^[^\s@]+@[^\s@]+\.[^\s@]+$/</span></span><span>;
    </span><span><span class="hljs-keyword">if</span></span><span> (!emailPattern.</span><span><span class="hljs-title function_">test</span></span><span>(email)) {
      emailError.</span><span><span class="hljs-property">textContent</span></span><span> = </span><span><span class="hljs-string">"Please enter a valid email address."</span></span><span>;
      valid = </span><span><span class="hljs-literal">false</span></span><span>;
    }

    </span><span><span class="hljs-keyword">if</span></span><span> (valid) {
      formMessage.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">remove</span></span><span>(</span><span><span class="hljs-string">"d-none"</span></span><span>, </span><span><span class="hljs-string">"alert-danger"</span></span><span>);
      formMessage.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">add</span></span><span>(</span><span><span class="hljs-string">"alert-success"</span></span><span>);
      formMessage.</span><span><span class="hljs-property">textContent</span></span><span> = </span><span><span class="hljs-string">" Request submitted successfully! We will contact you soon."</span></span><span>;
      contactForm.</span><span><span class="hljs-title function_">reset</span></span><span>();
    } </span><span><span class="hljs-keyword">else</span></span><span> {
      formMessage.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">remove</span></span><span>(</span><span><span class="hljs-string">"d-none"</span></span><span>, </span><span><span class="hljs-string">"alert-success"</span></span><span>);
      formMessage.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">add</span></span><span>(</span><span><span class="hljs-string">"alert-danger"</span></span><span>);
      formMessage.</span><span><span class="hljs-property">textContent</span></span><span> = </span><span><span class="hljs-string">" Please fix the errors above and try again."</span></span><span>;
    }
  });
}

</span><span><span class="hljs-comment">// ============ Availability page: refresh slot statuses ============</span></span><span>
</span><span><span class="hljs-keyword">const</span></span><span> randomizeSlotsBtn = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"randomizeSlotsBtn"</span></span><span>);
</span><span><span class="hljs-keyword">if</span></span><span> (randomizeSlotsBtn) {
  randomizeSlotsBtn.</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">"click"</span></span><span>, </span><span><span class="hljs-function">() =&gt;</span></span><span> {
    </span><span><span class="hljs-keyword">const</span></span><span> badges = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">querySelectorAll</span></span><span>(</span><span><span class="hljs-string">".status-badge"</span></span><span>);
    badges.</span><span><span class="hljs-title function_">forEach</span></span><span>(</span><span><span class="hljs-function">(<span class="hljs-params">b</span></span></span><span>) =&gt; {
      </span><span><span class="hljs-keyword">const</span></span><span> isFree = </span><span><span class="hljs-title class_">Math</span></span><span>.</span><span><span class="hljs-title function_">random</span></span><span>() &gt; </span><span><span class="hljs-number">0.5</span></span><span>;
      </span><span><span class="hljs-keyword">if</span></span><span> (isFree) {
        b.</span><span><span class="hljs-property">textContent</span></span><span> = </span><span><span class="hljs-string">"FREE"</span></span><span>;
        b.</span><span><span class="hljs-property">dataset</span></span><span>.</span><span><span class="hljs-property">status</span></span><span> = </span><span><span class="hljs-string">"free"</span></span><span>;
        b.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">remove</span></span><span>(</span><span><span class="hljs-string">"bg-danger"</span></span><span>);
        b.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">add</span></span><span>(</span><span><span class="hljs-string">"bg-success"</span></span><span>);
      } </span><span><span class="hljs-keyword">else</span></span><span> {
        b.</span><span><span class="hljs-property">textContent</span></span><span> = </span><span><span class="hljs-string">"OCCUPIED"</span></span><span>;
        b.</span><span><span class="hljs-property">dataset</span></span><span>.</span><span><span class="hljs-property">status</span></span><span> = </span><span><span class="hljs-string">"occupied"</span></span><span>;
        b.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">remove</span></span><span>(</span><span><span class="hljs-string">"bg-success"</span></span><span>);
        b.</span><span><span class="hljs-property">classList</span></span><span>.</span><span><span class="hljs-title function_">add</span></span><span>(</span><span><span class="hljs-string">"bg-danger"</span></span><span>);
      }
    });
  });
}
</span></span></code></div></div></pre>
